			<mvcSiteMapNode title="$resources:SiteMapLocalizations,ChangePasswordTitle" controller="Account" action="PasswordChange"/>
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,ChangePasswordTitle" controller="Account" action="ModifyLoginPwd"/>
			<!--<mvcSiteMapNode title="$resources:SiteMapLocalizations,Material" controller="Material" action="IndexDefault"/>-->
			<!--功能-->
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,FunctionTitle" controller="Function" action="Index"/>
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,FunctionTitle" controller="Function" action="IndexDefault"/>
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,RoleTitle" controller="Role" action="Index"/>
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,UserTitle" controller="User" action="Index"/>
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,DayEndReportTitle" area="Report" controller="DayEndReport" action="Index"/>
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,DayEndReportTitle" area="Report" controller="DayEndReport" action="DayEndSubmit"/>
		</mvcSiteMapNode>
 
		<!--Assignment 大结点-->
		<mvcSiteMapNode title="$resources:SiteMapLocalizations,AssignmentTitle" url="/" clickable="false">
		</mvcSiteMapNode>
    
		<!--Agreement 大结点-->
		<mvcSiteMapNode title="$resources:SiteMapLocalizations,AgreementTitle" url="/" clickable="false">
			<!--ProductRenewalLetterPrintingTitle-->
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,ProductRenewalLetterPrintingTitle" controller="ProductRenewalLetterPrinting" action="Index"/>
