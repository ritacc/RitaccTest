<?xml version="1.0" encoding="utf-8" ?>
<mvcSiteMap xmlns="http://mvcsitemap.codeplex.com/schemas/MvcSiteMap-File-3.0" enableLocalization="true">
	<mvcSiteMapNode title="">
		<!--System 大结点-->
		<mvcSiteMapNode title="$resources:SiteMapLocalizations,SystemTitle" url="/" clickable="false">
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,ShopTitle" controller="Shop" action="Index"/>
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,LoginTitle" controller="Account" action="Login" changeFrequency="Always" updatePriority="Normal"></mvcSiteMapNode>
