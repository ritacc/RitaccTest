			<mvcSiteMapNode title="$resources:SiteMapLocalizations,SupplierInvoiceTitle" controller="ApInvoice" action="Index"/>
			<!--ApCharge-->
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,ApChargeTitle" controller="ApInvoice" action="ApChargeList"/>
		</mvcSiteMapNode>
		
	 

		<!--Sales Memo 大结点-->
		<mvcSiteMapNode title="$resources:SiteMapLocalizations,SalesMemoTitle" url="/" clickable="false">
			
			<!--Sales Memo-->
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,SalesMemoTitle" controller="SalesMemo" action="Index"/>
			<mvcSiteMapNode title="$resources:SiteMapLocalizations,SalesReturnTitle" controller="SalesReturn" action="Index"/>
		</mvcSiteMapNode>

		 
	</mvcSiteMapNode>
</mvcSiteMap>
